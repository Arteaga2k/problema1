<!doctype html>
<html lang="es">
<head>
<meta charset="UTF-8" />
	<title>Problema 1</title>

<!-- Estilos css -->
{% block stylesheets %}
<link href="{{ constant('URL') }}assets/css/bootstrap.css"	type="text/css" rel="stylesheet" />
<link rel="stylesheet" href="//code.jquery.com/ui/1.11.2/themes/smoothness/jquery-ui.css">
{% endblock %}


</head>
<body>
	<div class="container-fluid">
		<nav class="navbar navbar-inverse" role="navigation">
			<div class="container-fluid">
				<!-- Brand and toggle get grouped for better mobile display -->
				<div class="navbar-header">
					<button type="button" class="navbar-toggle collapsed"
						data-toggle="collapse" data-target="#bs-example-navbar-collapse-1">
						<span class="sr-only">Toggle navigation</span> <span
							class="icon-bar"></span> <span class="icon-bar"></span> <span
							class="icon-bar"></span>
					</button>
					<a class="navbar-brand" href="{{ constant('URL') }}"> Logo aquí <!-- <img class="img-responsive" alt="Brand" src="{{ constant('URL') }}assets/img/kenollega.png"> --></a>
				</div>

				<!-- Collect the nav links, forms, and other content for toggling -->
				<div class="collapse navbar-collapse"
					id="bs-example-navbar-collapse-1">
					<ul class="nav navbar-nav">
						<li class="active"><a href="{{ constant('URL') }}envios/index">Envíos</a></li>
						<li><a href="{{ constant('URL') }}destinatarios/index">xxxxxx</a></li>
					</ul>
					<ul class="nav navbar-nav navbar-right">
					</ul>
				</div>
				<!-- /.navbar-collapse -->
			</div>
			<!-- /.container-fluid -->
		</nav>


		<!-- Contenido será rellenado por las diferentes vistas via Twig -->
		{% block content %} {% endblock %}




		<!-- Bloque javascript -->
	</div>
    {% block javascripts %}
     <script src="{{ constant('URL') }}assets/js/jquery-1.11.1.min.js" type="text/javascript"></script>  
        <script src="{{ constant('URL') }}assets/js/bootstrap.min.js" type="text/javascript"></script>          
        <script src="//code.jquery.com/ui/1.11.2/jquery-ui.js"></script>    
    {% endblock %} 
